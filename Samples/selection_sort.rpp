def main() {
	int arr[10],size=10,i = 0,j= 0,min,temp
	while i < size {
		get arr[i]
		i=i+1
	}
	i=0
	while i < size-1 {
		min = i
		j = i+1
		while j < size {
			if arr[j] < arr[min]
				min = j
			j=j+1
		}
		temp = arr[i]
		arr[i] = arr[min]
		arr[min] = temp
		i=i+1
	}

	i = 0
	while i < size {
		print arr[i]
		i=i+1
	}

}
